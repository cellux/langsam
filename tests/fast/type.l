(assert (type? Type))

(assert (type? Nil))
(assert (type? Boolean))
(assert (type? Integer))
(assert (type? Float))
(assert (type? String))
(assert (type? Symbol))
(assert (type? Keyword))
(assert (type? Opword))
(assert (type? Cons))
(assert (type? Vector))
(assert (type? Map))
(assert (type? Function))

(assert (= (type nil) Nil))
(assert (= (type true) Boolean))
(assert (= (type false) Boolean))
(assert (= (type 0) Integer))
(assert (= (type 0.0) Float))
(assert (= (type "foo") String))
(assert (= (type 'foo) Symbol))
(assert (= (type :foo) Keyword))
(assert (= (type %foo) Opword))
(assert (= (type '(a b)) Cons))
(assert (= (type '(a . b)) Cons))
(assert (= (type [:a :b]) Vector))
(assert (= (type {:a 1 :b 3 :c 8}) Map))
(assert (= (type do) Function))

(assert (nil? nil))
(assert (boolean? true))
(assert (boolean? false))
(assert (integer? 0))
(assert (float? 0.0))
(assert (string? "foo"))
(assert (symbol? 'foo))
(assert (keyword? :foo))
(assert (opword? %foo))
(assert (cons? '(a b)))
(assert (cons? '(a . b)))
(assert (proper-list? '(a b)))
(assert (proper-list? '()))
(assert (not (proper-list? '(a . b))))
(assert (not (proper-list? :a)))
(assert (vector? [:a :b]))
(assert (map? {:a 1 :b 3 :c 8}))
(assert (function? do))

(assert (macro? when))
(assert (special? do))

(assert (and (function? do) (special? do)))
(assert (and (function? when) (macro? when)))
